const calculate = (n1, n2, operator) => switch (operator) {
        case '+':
            return n1 + n2;
            break;
        case '-':
            return n1 - n2;
            break;
        case '*':
            return n1 * n2;
            break;
        case '/':
            return n1 / n2;
        default: 'no such operator'
    }

console.log(calculate(3,4, '+'))
